</head>

<body>
	<div class="wrapper">
		<div class="sidebar" data-image="@Url.Content("~/img/sidebar-5.jpg")">
			<div class="sidebar-wrapper">
				<div class="logo">
					<h1 class="sitename"><img src="~/image/HRoot_Logo-removebg-preview.png" style="max-height:46px !important" alt="Alternate Text" /></h1>
				</div>
				<ul class="nav">
					<li>
						<a class="nav-link" asp-action="Show_Employee" asp-controller="Manager">
							<i class="fa fa-user"></i>
							<p>Employee_data</p>
						</a>
					</li>
					<li>
						<a class="nav-link" href="@Url.Action("Addemployee", "Manager")">
							<i class="fa fa-user"></i>
							<p>Add Employee</p>
						</a>
					</li>
					<li>
						<a class="nav-link" asp-action="LeavingR" asp-controller="Manager">
							<i class="fa fa-table"></i>
							<p>
								Time-Off Request
							</p>
						</a>
					</li>
					<li>
						<a class="nav-link" asp-action="Logout" asp-controller="Manager">
							<i class="fa fa-sign-out"></i>
							<p>Logout</p>
						</a>
					</li>
					@* <li> *@
					@* 	<a class="nav-link" asp-action="Profile" asp-controller="Manager"> *@
					@* 		<i class="fa fa-user"></i> *@
					@* 		<p>Profile</p> *@
					@* 	</a> *@
					@* </li> *@
				</ul>
			</div>
		</div>
		<div class="main-panel">
			<!-- Navbar -->
			<div class="container">
				@RenderBody()
				</main>
			</div>
		</div>
		<!-- Content End -->
	</div>

	<!-- JS Files -->
	<script src="../assets/js/core/jquery.3.2.1.min.js" type="text/javascript"></script>
	<script src="../assets/js/core/popper.min.js" type="text/javascript"></script>
	<script src="../assets/js/core/bootstrap.min.js" type="text/javascript"></script>
	<script src="../assets/js/plugins/bootstrap-switch.js"></script>
	<script src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>
	<script src="../assets/js/plugins/chartist.min.js"></script>
	<script src="../assets/js/plugins/bootstrap-notify.js"></script>
	<script src="../assets/js/light-bootstrap-dashboard.js?v=2.0.0" type="text/javascript"></script>
	<script src="../assets/js/demo.js"></script>

	<script type="text/javascript">
		$(document).ready(function() {
			demo.initDashboardPageCharts();
			demo.showNotification();
		});
	</script>
	@RenderSection("scripts", required: false);
